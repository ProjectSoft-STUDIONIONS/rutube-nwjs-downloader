{
	"name": "RuTube-DL",
	"version": "5.1.0",
	"description": "Скачивание видео с RuTube на NW.js",
	"author": "ProjectSoft <projectsoft2009@yandex.ru>",
	"homepage": "https://github.com/ProjectSoft-STUDIONIONS/rutube-nwjs-downloader",
	"license": "MIT",
	"main": "index.html",
	"nodejs": true,
	"chromium-args": "--child-clean-exit --ignore-certificate-errors --disk-cache-size=1 --media-cache-size=1 --disable-web-security --use-strict=true --allow-running-insecure-content",
	"winIco": "favicon.ico",
	"page-cache": false,
	"webkit": {
		"plugin": true
	},
	"window": {
		"title": "RuTube-DL",
		"icon": "favicon.png",
		"exe_icon": "favicon.png",
		"mac_icon": "favicon.png",
		"show_in_taskbar": true,
		"kiosk_emulation": false,
		"id": "rutube-dl-nwjs",
		"position": "center",
		"resizable": true,
		"visible": true,
		"min_width": 800,
		"min_height": 520,
		"width": 800,
		"height": 520
	},
	"dependencies": {
		"m3u8-parser": "7.1.0",
		"nw-dialog": "1.0.7",
		"sanitize-filename": "1.6.3",
		"split-file": "2.3.0"
	}
}